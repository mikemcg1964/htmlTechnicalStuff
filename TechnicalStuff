Dim APIreq As New WinHttpRequest
With APIreq
	.Open "GET/POST", {Server URL}, False
	.SetRequestHeader "Authorization"/"AuthTokenName", "Basic {base64 username:password}"/Token
	.Send {+ xml body if required}
	Debug.Print .Status, .StatusText, .ResponseText
End With	

