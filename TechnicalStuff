 echo "select trid, \"Speaker 03\" from t_trader_speaker where speaker3=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 04\" from t_trader_speaker where speaker4=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 05\" from t_trader_speaker where speaker5=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 06\" from t_trader_speaker where speaker6=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 07\" from t_trader_speaker where speaker7=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 08\" from t_trader_speaker where speaker8=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 09\" from t_trader_speaker where speaker9=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 10\" from t_trader_speaker where speaker10=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 11\" from t_trader_speaker where speaker11=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 12\" from t_trader_speaker where speaker12=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 13\" from t_trader_speaker where speaker13=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 14\" from t_trader_speaker where speaker14=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
 echo "select trid, \"Speaker 15\" from t_trader_speaker where speaker15=$1 order by trid" | M_XSQL.MACH -s -w2000 >> /tmp/speaker_info
