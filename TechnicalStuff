#!/bin/bash
##=Begin Shell Script Preamble=======================================================
##
## Alarm Monitoring Script
## Mike McGowan 24th April 2012
## 9-5-12 v.2 added colours
##
##
##=End Shell Script Preamble========================================================
cd /usr/sx/db
clear
echo -e "\e[1;01m\033[36mAlarm Monitor by Mike McGowan v.2 - Now in Technicolor! 9th May 2012\033[37m"

# starttime=`echo "select max(alarm_last) from t_alarm_log" | M_XSQL.MACH -s -w2000 | grep 2012`

starttime="2012-05-14 10:41:41"

while true
do
echo "select 'MMcG_BankSt:>' as m,alarm_last,'cab/shelf/slot/port =',cabinet as ca, shelf as sf,slot as st,port as pt,a.description,alarm_id,b.description as alarmstate,'count =' as coun,alarm_count from t_alarm_log,t_system_symbols a,t_system_symbols b where alarm_id not in (339,340,344) and alarm_last > '$starttime' and a.name_space='alarm_id' and a.value=alarm_id and b.name_space='alarm_state' and b.value=alarm_state order by alarm_last" | M_XSQL.MACH -w2000 -s | grep 2012 | sed 's/ /./g' > /usr/sx/db/MIKE/ALARM.TXT

alarmstr=`cat /usr/sx/db/MIKE/ALARM.TXT`

for i in $alarmstr

 do

 if echo $i | grep -q Unattend
  then
  echo -e "\a\033[31m$i\033[37m"
 fi

 if echo $i | grep -q Attend
  then
  echo -e "\033[32m$i\033[37m"
 fi

 if echo $i | grep -q Report
  then
  echo "$i"
 fi

done

starttime=`echo "select max(alarm_last) from t_alarm_log" | M_XSQL.MACH -s -w2000 | grep 2012`

sleep 30
done

echo

