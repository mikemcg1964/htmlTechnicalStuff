Delete dupes:
	DELETE * FROM tablea a WHERE a.field <>
     (SELECT MAX(b.field) FROM tableb b
      WHERE b.field2 = a.field2);

SaveAsText, acForm, "FormName", "C:\Downloads\Form.txt"
LoadFromText, acForm, "FormName", "C:\Downloads\Form.txt"

sSQL = "SELECT * FROM myTable"
Set rst = CurrentDB.OpenRecordset(sSQL)
With rst
	Do While Not .EOF
		Debug.Print rst!Field1 ' OR:-
		Debug.Print .Fields("Field1")
		.MoveNext
	Loop
End With

Sub quickcount(table_name)
	sSQL = "SELECT Count(*) AS [myCount] FROM [" & table_name & "]"
	Set rst = CurrentDb.OpenRecordSet(sSQL)
	Debug.Print "count of records in " & table_name & " =" & rst!myCount
	Set rst = Nothing
End Sub


Rename FieldName:
Set db = CurrentDb
db.TableDefs("myTable").Fields("myFieldName").Name = "NewFieldName"
db.Close

<u>NPP</u>
