<u>MySQL</u>
mysqldump -u{user} -p{password} {db_name} > backup.sql
mysql -u{user} -p{password} {db_name} < backup.sql
mysqldump -u{user} -p{password} {db_name} {table1} {table2} > backup.sql
mysqldump -u{user} -p{password} {db_name} --ignore-table={db_name}.{table1} --ignore-table={db_name}.{table2} > backup.sql
mysql> use mysql;
mysql> create database {db_name}
mysql> create user '{user}'@'%' identified by '{password}';
mysql> grant all privileges on {db_name}.* to '{user}'@'%';
mysql> show grants for {user};
show databases, show schemas

