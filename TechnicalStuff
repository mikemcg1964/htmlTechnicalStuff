script LANGUAGE = "Javascript">
munf=new Array("January","February","March","April","May",
"June","July","August","September","October",
"November","December");
weekday=new Array("Sun","Mon","Tues","Wednes","Thurs","Fri","Satur")
mydate=new Date();


suffix="th"

if ((mydate.getDate()==1)||(mydate.getDate()==21)||(mydate.getDate()==31))
	suffix="st"
if ((mydate.getDate()==2)||(mydate.getDate()==22))
	suffix="nd"
if ((mydate.getDate()==3)||(mydate.getDate()==23))
	suffix="rd"

yar=mydate.getYear()
yar=yar + 1900

manufacturer=navigator.appName
//document.write(manufacturer)
if (manufacturer.indexOf('Microsoft')>=0)
yar=yar - 1900
;
document.write(weekday[mydate.getDay()]+"day the "+mydate.getDate()+suffix+" "+munf[mydate.getMonth()]+" "+yar)
