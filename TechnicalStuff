<u>MS Project</u>
VBA:
Sub addTsk()
	Dim tsk As Task

	Set tsk = ActiveProject.Tasks.Add("Task Name")
	tsk.Start = "dd/mm/yyyy hh:mm"
	'tsk.Finish = "dd/mm/yyyy hh:mm"
	tsk.Duration = "8 hrs"
	tsk.ResourceNames = "Mike"
	tsk.Notes = "Hello World"
	tsk.Predecessors = tsk.ID - 1 ' & "SS+1h"
	tsk.Calendar = "24 Hours"
End Sub

Sub addTskFromCSV()
	Dim tsk As Task, FileNum As Integer, DataLine As String, Dim LineItems As Variant, myDate As DATETIME
	
	'example of c:\data.csv:
	'13/06/2020 08:00, 4, task description
	
	FileNum = FreeFile()
	Open "c:\data.csv" For Input As #FileNum
	
	While Not EOF(FileNum)
		Line Input #FileNum, Dataline
		LineItems = Split(Dataline, ",")
		Set tsk = ActiveProject.Tasks.Add(LineItems(2))
		tsk.ResourceNames = LineItems(2) ' Note: I use this to show task description against bar in Gantt
		tsk.Start = LineItems(0)
		tsk.Duration = LineItems(1) & " hrs"
		myDate = Left(LineItems(0), 10)
		If Now() > myDate Then tsk.PercentComplete = "100"
	Wend
End Sub
	

