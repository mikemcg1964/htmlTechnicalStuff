Sub delSheets()
	Dim Sh As Worksheet
	Application.DisplayAlerts = False
	For Each Sh in Worksheets
		If Sh.Name <> ActiveSheet.Name Then Sh.Delete
	Next Sh
	Application.DisplayAlerts = True
End Sub
	
