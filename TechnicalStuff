 echo
 echo "LAC $1 is on the following speakers"
 echo "select system_descriptor from t_line where line_lac=$1" | M_XSQL.MACH -s
 echo
 echo "TrID   Speaker"
 echo "----   ----------"
 sort -n /tmp/speaker_info | grep Speak
 # rm /tmp/speaker_info
fi

#!/bin/sh
##=Begin Shell Script Preamble=======================================================
##
## Script to show trids that aren't recorded
## v.1 July 19th 2011
## usage examples: no_rec
##
##=End Shell Script Preamble========================================================
user_inp=" "
cd /usr/sx/db
echo
echo "No Record Script by Mike McGowan - v.1 19th July 2011"
echo "the following trids have no record map set"
echo
echo "SELECT a.trid, trader_name, b.trid FROM t_trader a, OUTER t_ip_record_map b WHERE b.trid=a.trid ORDER BY a.trid"| M_XSQL.MACH -s -w2000 > /tmp/no_rec.txt
head -2 /tmp/no_rec.txt
cat /tmp/no_rec.txt | grep -i "32768"
rm -r /tmp/no_rec.txt
echo


#!/bin/sh
##=Begin Shell Script Preamble=======================================================
##
## Line Info script by Mike McGowan
## v.1 July 25th 2011
## usage examples: line_info 1234
##
##=End Shell Script Preamble========================================================
user_inp=" "
cd /usr/sx/db
if [ $# = 0 ]; then
 user_inp = "1024"
else
  user_inp=$1
fi
echo
echo "Line Information Script by Mike McGowan - v.1 25th July 2011"
echo
echo "select line_lac as llac, system_descriptor as descriptor, customer_field_1 as groop, customer_field_2 as trader, circuit_id from t_line order by line_lac"| M_XSQL.MACH -s -w2000 > /tmp/line_info.txt
echo "----   -------------------------------   ---------------   ---------------   ----------"
echo "LAC    Descriptor                        Group             Trader Name       Circuit ID"
echo "----   -------------------------------   ---------------   ---------------   ----------"
cat /tmp/line_info.txt | grep -i "$user_inp"
echo "----   -------------------------------   ---------------   ---------------   ----------"
rm -r /tmp/line_info.txt
echo

